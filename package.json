{
  "name": "3dtiles-geo-transform",
  "version": "1.0.0",
  "description": "Geographic coordinate transformation library for 3D Tiles with Three.js and React support",
  "keywords": [
    "3d-tiles",
    "geographic-coordinates",
    "coordinate-transformation",
    "cesium",
    "three.js",
    "ecef",
    "wgs84",
    "gltf",
    "react",
    "geospatial",
    "cesium-rtc"
  ],
  "homepage": "https://github.com/SohMitian/3dtiles-geo-transform#readme",
  "bugs": {
    "url": "https://github.com/SohMitian/3dtiles-geo-transform/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SohMitian/3dtiles-geo-transform.git"
  },
  "license": "MIT",
  "author": "SohMitian",
  "main": "dist/index.cjs",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.esm.js"
    }
  },
  "directories": {
    "example": "example",
    "test": "tests"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.30",
    "@types/react": "^19.1.12",
    "@types/three": "^0.179.0",
    "jest": "^29.7.0",
    "rollup": "^4.13.0",
    "rollup-plugin-dts": "^6.1.0",
    "ts-jest": "^29.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3"
  },
  "peerDependencies": {
    "@react-three/fiber": ">=8.0.0",
    "react": ">=16.8.0",
    "three": ">=0.150.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    },
    "@react-three/fiber": {
      "optional": true
    }
  }
}
